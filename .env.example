# Generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=****

# The following keys below are automatically created and
# added to your environment when you deploy on Vercel

# Instructions to create an AI Gateway API key here: https://vercel.com/ai-gateway 
# API key required for non-Vercel deployments
# For Vercel deployments, OIDC tokens are used automatically
# https://vercel.com/ai-gateway
AI_GATEWAY_API_KEY=****


# Instructions to create a Vercel Blob Store here: https://vercel.com/docs/vercel-blob
BLOB_READ_WRITE_TOKEN=****

# Instructions to create a PostgreSQL database here: https://vercel.com/docs/postgres
POSTGRES_URL=****


# Instructions to create a Redis store here:
# https://vercel.com/docs/redis
REDIS_URL=****

# Chat backend selector: claude_proxy (default) | gateway
CHAT_BACKEND=claude_proxy

# Trigger/stream timeout settings (30 minutes)
TRIGGER_FUND_CHAT_MAX_DURATION_SECONDS=1800
TRIGGER_UPSTREAM_TIMEOUT_MS=1800000
TRIGGER_STREAM_READ_TIMEOUT_SECONDS=1800
CHAT_API_DEBUG_VERBOSE=false
TRIGGER_TASK_DEBUG_VERBOSE=false

# External backend base URL (use your Cloudflare Tunnel URL in production)
CLAUDE_CODE_API_BASE=http://127.0.0.1:15722
CLAUDE_CODE_GATEWAY_TOKEN=****
CLAUDE_CODE_MODEL=gpt-5-codex
# Optional: shared internal key (must match backend/.env.*) for trusted precheck bypass
INTERNAL_TASK_KEY=

# Optional: public web URL for absolute artifact links (e.g. https://backtrack.example.com)
PUBLIC_APP_URL=

# Proxy safety controls
BLOCK_URL_INPUT=true
URL_BLOCKED_REPLY=Sorry, I cannot access or download links due to security policy. Please paste text directly.
REDACT_SENSITIVE_OUTPUT=true

# Optional quota controls
DISABLE_CHAT_DAILY_QUOTA=false
CHAT_MAX_MESSAGES_PER_DAY_GUEST=20
CHAT_MAX_MESSAGES_PER_DAY_REGULAR=50

# Optional analytics (Umami)
NEXT_PUBLIC_UMAMI_WEBSITE_ID=
NEXT_PUBLIC_UMAMI_SRC=https://cloud.umami.is/script.js
